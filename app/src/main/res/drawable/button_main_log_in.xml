<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/blanched_almond"/>
            <stroke android:color="@color/skobeloff" android:width="5px"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/honey_dew"/>
            <stroke android:color="@color/amaranth_pink" android:width="5px"/>
            <corners android:radius="50dp"/>
        </shape>
    </item>
</selector>